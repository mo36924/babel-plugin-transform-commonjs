{
  "name": "@mo36924/babel-plugin-transform-commonjs",
  "version": "1.0.0",
  "description": "babel-plugin-transform-commonjs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mo36924/babel-plugin-transform-commonjs.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mo36924/babel-plugin-transform-commonjs/issues"
  },
  "homepage": "https://github.com/mo36924/babel-plugin-transform-commonjs#readme",
  "publishConfig": {
    "access": "public"
  },
  "browser": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "browser": "./dist/index.mjs",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "run-s clean test format type bundle",
    "clean": "del dist",
    "format": "prettier --write '**/*.{js,jsx,ts,tsx,json}'",
    "type": "tsc -p ./tsconfig.type.json",
    "bundle": "rollup -c",
    "test": "jest"
  },
  "prettier": {
    "printWidth": 120,
    "trailingComma": "all"
  },
  "jest": {
    "preset": "ts-jest",
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.test.json"
      }
    },
    "moduleNameMapper": {
      "^~/(.*)$": "<rootDir>/src/$1"
    },
    "testMatch": [
      "**/__tests__/**/*.[jt]s?(x)",
      "**/?(*.)+(spec|test).[jt]s?(x)"
    ],
    "moduleFileExtensions": [
      "tsx",
      "ts",
      "jsx",
      "js",
      "mjs",
      "json",
      "node"
    ],
    "snapshotSerializers": [
      "@mo36924/jest-snapshot-serializer-babel"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@mo36924/jest-snapshot-serializer-babel": "^1.0.0",
    "@rollup/plugin-typescript": "^8.1.0",
    "@types/babel__core": "^7.1.12",
    "del-cli": "^3.0.1",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "rollup": "^2.35.1",
    "ts-jest": "^26.4.4",
    "tslib": "^2.0.3",
    "typescript": "^4.1.3"
  },
  "peerDependencies": {
    "@babel/core": "^7.12.10",
    "@types/babel__core": "^7.1.12"
  }
}
